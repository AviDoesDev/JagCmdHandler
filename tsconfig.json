{
    "compilerOptions": {
        "target": "EsNext",
        "module": "CommonJs",
        "moduleResolution": "Node",
        "declaration": true,
        "outDir": "./lib",
        "strict": true,
        "strictNullChecks": false,
        "esModuleInterop": true,
        "pretty": true,
        "removeComments": false,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "typeRoots": ["node_modules/@types"],
    },
    "include": ["src/**/**/*"]
  }
{
  "name": "jag-cmd-handler",
  "version": "0.0.3-lts",
  "description": "An advanced djs 13 command handler.",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "lint": "eslint ./src --ext .ts"
  },
  "author": "JAGUARAVI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JAGUARAVI/JagCmdHandler.git"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "linebreak-style": "off",
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "chalk": "^4.1.2",
    "discord.js": "^13.6.0",
    "moment": "^2.29.1",
    "pretty-ms": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "typescript": "^4.3.5"
  }
}
